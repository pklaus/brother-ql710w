# Install file for brother-ql710w-lpr

post_install() {
        useradd -s /bin/false -g lp lp
        mkdir -p /var/spool/lpd/ql710w
        /opt/brother/PTouch/ql710w/inf/setupPrintcappt1 ql710w -i USB
}

pre_remove() {
        rm -rf /var/spool/lpd/ql710w
        /opt/brother/PTouch/ql710w/inf/setupPrintcappt1 ql710w -e
}
